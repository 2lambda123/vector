{
  "name": "vrl",
  "rules": {
    "program": {
      "type": "SYMBOL",
      "name": "exprs"
    },
    "exprs": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expr"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_expr_end"
              },
              {
                "type": "SYMBOL",
                "name": "expr"
              }
            ]
          }
        }
      ]
    },
    "expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "query"
        }
      ]
    },
    "_expr_end": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        }
      ]
    },
    "_integer": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[0-9_]+"
        }
      ]
    },
    "integer_literal": {
      "type": "SYMBOL",
      "name": "_integer"
    },
    "float_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_integer"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "."
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "FIELD",
            "name": "fraction",
            "content": {
              "type": "PATTERN",
              "value": "[0-9_]+"
            }
          }
        }
      ]
    },
    "query": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "local_query"
        }
      ]
    },
    "local_query": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "local_variable"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "query_segments"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "local_variable": {
      "type": "PATTERN",
      "value": "[0-9]*[a-zA-Z_][0-9a-zA-Z_]*"
    },
    "query_segments": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "SYMBOL",
            "name": "query_segment"
          }
        ]
      }
    },
    "query_segment": {
      "type": "PATTERN",
      "value": "[0-9]*[a-zA-Z_@][0-9a-zA-Z_@]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

